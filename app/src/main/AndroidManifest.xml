<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.booksalonappointment">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookSalonAppointment"
        tools:targetApi="31">
        <activity
            android:name=".view.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".view.AboutUSActivity"
            android:exported="false"
            android:parentActivityName=".view.DashBoardActivity" />
        <activity
            android:name=".view.WorkingHoursActivity"
            android:exported="false"
            android:parentActivityName=".view.DashBoardActivity" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/actions" />

        <activity
            android:name=".view.DashBoardActivity"
            android:exported="false" />
        <activity
            android:name=".view.ServiceActivity"
            android:exported="false" />
        <activity
            android:name=".view.DashBoardActivityWhenLoggedIn"
            android:exported="false" />
        <activity
            android:name=".view.HomeScreenActivity"
            android:exported="false" />
        <activity
            android:name=".view.LogInActivity"
            android:exported="false" />
        <activity
            android:name=".view.LogOutActivity"
            android:exported="false" />
        <activity
            android:name=".view.OfferActivity"
            android:exported="false"
            android:parentActivityName=".view.DashBoardActivity" />
        <activity
            android:name=".view.HomeCareTipsActivity"
            android:exported="false"
            android:parentActivityName=".view.DashBoardActivity" />
        <activity
            android:name=".view.RegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".view.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>